// Brute Force: 
class Solution {
public:
    int beauty(vector <int> &freq) {
        int mini = INT_MAX; 
        int maxi = 0; 
        for (int i = 0; i < freq.size(); i++) {
            if (freq[i] > 0) {
                maxi = max(maxi, freq[i]); 
                mini = min(mini, freq[i]);  
            }    
        }
        return abs(maxi - mini); 
    }
    int beautySum(string s) {
        int sum = 0; 
        int res = 0; 
        for (int i = 0; i < s.size(); i++) {
            vector <int> freq (26, 0); 
            for (int j = i; j < s.size(); j++) {
                freq[s[j] - 'a']++; 
                sum += beauty(freq); 
            }
        }
        return sum;
    }
};
